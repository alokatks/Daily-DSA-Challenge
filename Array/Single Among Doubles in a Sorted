class Solution {
    int single(int[] arr) {
        HashMap<Integer,Integer>map=new HashMap<>();
        for(int ele:arr){
            if(map.containsKey(ele)){
            int freq=map.get(ele);
            map.put(ele,freq+1);
        }else{
            map.put(ele,1);
        }
        }
        for(int ele:arr){
            if(map.get(ele)==1)
            return ele;
        }
  return -1;
   
    }
}
